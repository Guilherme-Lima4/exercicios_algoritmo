// Seção de Declarações 
var
num,an,su: inteiro
inicio
escreval ("digite um número:")
leia (num)
an <- num - 1
su <- num + 1
escreval ("o antecessor do número inserido é:", an)
escreval ("o sucessor do número inserido é:", su)
fimalgoritmo
